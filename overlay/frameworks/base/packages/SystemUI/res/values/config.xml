<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <!-- Doze: does this device support STATE_DOZE?  -->
    <bool name="doze_display_state_supported">true</bool>

    <!-- Doze: check proximity sensor before pulsing? -->
    <bool name="doze_proximity_check_before_pulse">false</bool>

    <!-- Color of the FOD view -->
    <color name="config_fodColor">#00ffffff</color>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">7</integer>
    <integer name="config_maxVisibleNotificationIconsWhenDark">8</integer>

    <!-- Whether to show biometric help only when failed -->
    <bool name="config_biometricHelpShowOnlyWhenFailed">true</bool>

    <!-- Enable face auth only when swiping security view -->
    <bool name="config_faceAuthOnlyOnSecurityView">true</bool>

</resources>
